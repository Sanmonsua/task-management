{
	"expo": {
		"name": "Tasks-io",
		"slug": "tasks-io",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "cover",
			"backgroundColor": "#ffffff"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.sanmonsua.tasksrn",
			"config": {
				"googleSignIn": {
					"reservedClientId": "com.googleusercontent.apps.948857622296-j32kb6pgeapsmp46ejc6fl3s1lm9hn44"
				}
			},
			"googleServicesFile": "./GoogleService-Info.plist"
		},
		"web": {
			"favicon": "./assets/favicon.png"
		},
		"description": "In this project I have developed an app for tasks management using redux.",
		"githubUrl": "https://github.com/Sanmonsua/task-management/",
		"android": {
			"package": "com.sanmonsua.tasksrn",
			"googleServicesFile": "./google-services.json"
		},
		"entryPoint": "node_modules/expo/AppEntry.js"
	}
}
